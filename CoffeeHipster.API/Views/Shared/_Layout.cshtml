    <script src="~/App/Framework/chFrameworkModule.js"></script>
    <script src="~/App/Framework/chFrameworkController.js"></script>
    <script src="~/App/Framework/chFrameworkDirective.js"></script>

    <script src="~/App/Framework/Messages/Services/messagesCRUDService.js"></script>
    <script src="~/App/Framework/Dashboard/Services/dashboardCRUDService.js"></script>
    <script src="~/App/Framework/Create/Services/createCRUDService.js"></script>
    <script src="~/App/Framework/Content/Services/contentCRUDService.js"></script>


    <script src="~/App/appModule.js"></script>
    <script src="~/App/appConfig.js"></script> 
    <script src="~/App/appRouteConfig.js"></script>

    



</head>
<body>
   
    <div>
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
